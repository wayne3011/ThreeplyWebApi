{
  "ThreeplyDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "ThreeplyDB",
    "DatabaseRespondTimeoutMS": 0
  },
  "Groups": {
    "GroupsCollectionName": "Groups"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "ThreeplyWebApi.Controllers.AuthenticationScheme": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
              }
            }
          ]
        }
      },
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://localhost:27017/ThreeplyDB",
          "collectionName": "Log",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000"
        }
      }
    ]
  },
  "Parsing": {
    "maiScheduleUrl": "https://mai.ru/education/studies/schedule",
    "scheduleUpdateTime": [ "19:00", "2:30", "15:00" ]
  },
  "AllowedHosts": "*"
}
